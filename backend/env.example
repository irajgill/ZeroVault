# Server
PORT=4000
NODE_ENV=development

# Sui Network
SUI_NETWORK=testnet
PACKAGE_ID=
MARKETPLACE_ID=
PROOF_REGISTRY_ID=
VK_OBJECT_ID=

# Dev: ZK bypass (returns isValid=true and skips on-chain verify in purchase confirm)
ZK_FAKE_VALID=0

# Walrus
WALRUS_AGGREGATOR_URL=https://aggregator.walrus-testnet.walrus.space
WALRUS_PUBLISHER_URL=https://publisher.walrus-testnet.walrus.space
# Optional aliases used by backend code (either pair works)
WALRUS_AGGREGATOR=https://aggregator.walrus-testnet.walrus.space
WALRUS_PUBLISHER=https://publisher.walrus-testnet.walrus.space
# Optional explicit upload URL or path for publisher
# If your publisher requires a fixed endpoint, set one of:
# WALRUS_PUBLISHER_UPLOAD_URL=https://publisher.walrus-testnet.walrus.space/v1/store
# or WALRUS_PUBLISHER_PATH=/v1/store
WALRUS_PUBLISHER_PATH=
WALRUS_PUBLISHER_UPLOAD_URL=

# Nautilus
NAUTILUS_URL=http://localhost:3000
MIN_QUALITY_THRESHOLD=60
# Optional: pin Nautilus attestation public key (base64, 32 bytes Ed25519)
NAUTILUS_ATTEST_PUBKEY=

# PostgreSQL (choose either DATABASE_URL or discrete vars)
# DATABASE_URL=postgres://user:password@localhost:5432/zkdatavault
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=
PGDATABASE=zkdatavault
# Set to 1 to enable SSL (Heroku/Railway etc.)
PGSSL=0



